<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.my_cars_sqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_ferrari"
        android:label="MY_Cars_SqLite2"
        android:roundIcon="@mipmap/ic_launcher_ferrari_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".crud.Details"/>
        <activity android:name=".crud.EditRecord"/>
        <activity android:name=".crud.Insert"/>
        <activity android:name=".crud.ListAll"/>

    </application>

</manifest>